{
  "name": "naniscript",
  "rules": {
    "script": {
      "type": "SEQ",
      "members": [
        {
          "type": "REPEAT",
          "content": {
            "type": "SEQ",
            "members": [
              {
                "type": "SYMBOL",
                "name": "line"
              },
              {
                "type": "PATTERN",
                "value": "[\\n\\r]+"
              }
            ]
          }
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "line"
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "line": {
      "type": "SEQ",
      "members": [
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "PATTERN",
              "value": "[ \\t]*"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "comment"
            },
            {
              "type": "SYMBOL",
              "name": "define"
            },
            {
              "type": "SYMBOL",
              "name": "label"
            },
            {
              "type": "SYMBOL",
              "name": "command"
            },
            {
              "type": "SYMBOL",
              "name": "generic_text"
            }
          ]
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "PATTERN",
              "value": "[ \\t]*"
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "comment": {
      "type": "PATTERN",
      "value": ";[^\\r\\n]*"
    },
    "define": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": ">"
        },
        {
          "type": "SYMBOL",
          "name": "define_key"
        },
        {
          "type": "PATTERN",
          "value": "[ \\t]+"
        },
        {
          "type": "SYMBOL",
          "name": "define_value"
        }
      ]
    },
    "define_key": {
      "type": "PATTERN",
      "value": "[a-zA-Z0-9_]+"
    },
    "define_value": {
      "type": "PATTERN",
      "value": "[^{}\\r\\n]+"
    },
    "label": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "#"
        },
        {
          "type": "PATTERN",
          "value": "[ \\t]*"
        },
        {
          "type": "SYMBOL",
          "name": "label_id"
        }
      ]
    },
    "label_id": {
      "type": "PATTERN",
      "value": "[a-zA-Z0-9_]+"
    },
    "command": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "@"
        },
        {
          "type": "SYMBOL",
          "name": "command_id"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "command_nameless_param"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "command_param"
          }
        }
      ]
    },
    "command_id": {
      "type": "PATTERN",
      "value": "[a-zA-Z0-9_]+"
    },
    "command_param": {
      "type": "SEQ",
      "members": [
        {
          "type": "PATTERN",
          "value": "[ \\t]+"
        },
        {
          "type": "SYMBOL",
          "name": "command_param_id"
        },
        {
          "type": "SYMBOL",
          "name": "command_param_value"
        }
      ]
    },
    "command_nameless_param": {
      "type": "SEQ",
      "members": [
        {
          "type": "PATTERN",
          "value": "[ \\t]+"
        },
        {
          "type": "SYMBOL",
          "name": "command_param_value"
        }
      ]
    },
    "command_param_id": {
      "type": "PATTERN",
      "value": "[a-zA-Z0-9_]+:"
    },
    "command_param_value": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SEQ",
          "members": [
            {
              "type": "STRING",
              "value": "\""
            },
            {
              "type": "CHOICE",
              "members": [
                {
                  "type": "REPEAT1",
                  "content": {
                    "type": "PATTERN",
                    "value": "(\\\\.|[^\"\\r\\n\\[\\]{}])+"
                  }
                },
                {
                  "type": "SYMBOL",
                  "name": "expression"
                }
              ]
            },
            {
              "type": "STRING",
              "value": "\""
            }
          ]
        },
        {
          "type": "REPEAT1",
          "content": {
            "type": "CHOICE",
            "members": [
              {
                "type": "PATTERN",
                "value": "[^\"\\s\\r\\n:\\[\\]{}@]+"
              },
              {
                "type": "SYMBOL",
                "name": "expression"
              }
            ]
          }
        }
      ]
    },
    "generic_text": {
      "type": "PREC",
      "value": -1,
      "content": {
        "type": "REPEAT1",
        "content": {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "expression"
            },
            {
              "type": "SYMBOL",
              "name": "inlined_command"
            },
            {
              "type": "PATTERN",
              "value": "[^\\r\\n\\[\\]{}]+"
            }
          ]
        }
      }
    },
    "inlined_command": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "["
        },
        {
          "type": "SYMBOL",
          "name": "command_id"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "command_nameless_param"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "command_param"
          }
        },
        {
          "type": "STRING",
          "value": "]"
        }
      ]
    },
    "expression": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "{"
        },
        {
          "type": "PATTERN",
          "value": "[^\\r\\n{}]*"
        },
        {
          "type": "STRING",
          "value": "}"
        }
      ]
    }
  },
  "extras": [],
  "conflicts": [],
  "externals": [],
  "inline": [],
  "supertypes": []
}

