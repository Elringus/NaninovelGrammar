{
  "name": "naniscript",
  "rules": {
    "program": {
      "type": "REPEAT",
      "content": {
        "type": "CHOICE",
        "members": [
          {
            "type": "SYMBOL",
            "name": "comment"
          },
          {
            "type": "SYMBOL",
            "name": "define"
          },
          {
            "type": "SYMBOL",
            "name": "command_line"
          },
          {
            "type": "SYMBOL",
            "name": "label"
          },
          {
            "type": "SYMBOL",
            "name": "inline_command"
          }
        ]
      }
    },
    "label": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "label_sign"
        },
        {
          "type": "SYMBOL",
          "name": "_hor_space"
        },
        {
          "type": "SYMBOL",
          "name": "label_name"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "_hor_space"
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "label_name": {
      "type": "PATTERN",
      "value": "\\w+"
    },
    "label_sign": {
      "type": "STRING",
      "value": "#"
    },
    "comment": {
      "type": "SEQ",
      "members": [
        {
          "type": "PATTERN",
          "value": ";.*"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "_hor_space"
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "define": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "define_sign"
        },
        {
          "type": "SYMBOL",
          "name": "define_name"
        },
        {
          "type": "SYMBOL",
          "name": "_hor_space"
        },
        {
          "type": "SYMBOL",
          "name": "define_val"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "_hor_space"
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "define_name": {
      "type": "PATTERN",
      "value": "\\w+"
    },
    "define_val": {
      "type": "PATTERN",
      "value": "[^\\n\\[\\]]+"
    },
    "define_sign": {
      "type": "STRING",
      "value": ">"
    },
    "command_line": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_non_inlined_name"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SEQ",
              "members": [
                {
                  "type": "SYMBOL",
                  "name": "_hor_space"
                },
                {
                  "type": "SYMBOL",
                  "name": "unnamed_arg"
                }
              ]
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SEQ",
            "members": [
              {
                "type": "SYMBOL",
                "name": "_hor_space"
              },
              {
                "type": "SYMBOL",
                "name": "named_arg"
              }
            ]
          }
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "_hor_space"
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "inline_command": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "["
        },
        {
          "type": "SYMBOL",
          "name": "command_name"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SEQ",
              "members": [
                {
                  "type": "SYMBOL",
                  "name": "_hor_space"
                },
                {
                  "type": "SYMBOL",
                  "name": "unnamed_arg"
                }
              ]
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SEQ",
            "members": [
              {
                "type": "SYMBOL",
                "name": "_hor_space"
              },
              {
                "type": "SYMBOL",
                "name": "named_arg"
              }
            ]
          }
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "_hor_space"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "STRING",
          "value": "]"
        }
      ]
    },
    "_non_inlined_name": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "@"
        },
        {
          "type": "SYMBOL",
          "name": "command_name"
        }
      ]
    },
    "command_name": {
      "type": "PATTERN",
      "value": "\\w+"
    },
    "unnamed_arg": {
      "type": "TOKEN",
      "content": {
        "type": "CHOICE",
        "members": [
          {
            "type": "PATTERN",
            "value": "[^\"\\n\\s:\\[\\]@]+"
          },
          {
            "type": "SEQ",
            "members": [
              {
                "type": "STRING",
                "value": "\""
              },
              {
                "type": "PATTERN",
                "value": "[^\\n\\[\\]]*"
              },
              {
                "type": "STRING",
                "value": "\""
              }
            ]
          }
        ]
      }
    },
    "named_arg": {
      "type": "TOKEN",
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "PATTERN",
            "value": "[a-zA-Z0-9_]+"
          },
          {
            "type": "STRING",
            "value": ":"
          },
          {
            "type": "CHOICE",
            "members": [
              {
                "type": "PATTERN",
                "value": "[^\\n\\s:\\[\\]]+"
              },
              {
                "type": "SEQ",
                "members": [
                  {
                    "type": "STRING",
                    "value": "\""
                  },
                  {
                    "type": "PATTERN",
                    "value": "[^\\n\\[\\]]*"
                  },
                  {
                    "type": "STRING",
                    "value": "\""
                  }
                ]
              }
            ]
          }
        ]
      }
    },
    "_hor_space": {
      "type": "PATTERN",
      "value": "[ \\t]+"
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    }
  ],
  "conflicts": [],
  "externals": [],
  "inline": [],
  "supertypes": []
}

