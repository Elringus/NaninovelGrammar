{
  "name": "naniscript",
  "rules": {
    "fragment": {
      "type": "REPEAT",
      "content": {
        "type": "SYMBOL",
        "name": "_node"
      }
    },
    "_node": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "comment"
        },
        {
          "type": "SYMBOL",
          "name": "define"
        },
        {
          "type": "SYMBOL",
          "name": "label"
        },
        {
          "type": "SYMBOL",
          "name": "command"
        },
        {
          "type": "SYMBOL",
          "name": "inlined_command"
        },
        {
          "type": "SYMBOL",
          "name": "generic_text"
        },
        {
          "type": "SYMBOL",
          "name": "expression"
        }
      ]
    },
    "comment": {
      "type": "PATTERN",
      "value": ";[^\\r\\n]*"
    },
    "expression": {
      "type": "TOKEN",
      "content": {
        "type": "PREC",
        "value": 1,
        "content": {
          "type": "PATTERN",
          "value": "{[^{}\\r\\n]+}"
        }
      }
    },
    "generic_text": {
      "type": "TOKEN",
      "content": {
        "type": "PREC",
        "value": -1,
        "content": {
          "type": "PATTERN",
          "value": "[^\\r\\n\\[\\]{}]+"
        }
      }
    },
    "define": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": ">"
        },
        {
          "type": "SYMBOL",
          "name": "define_key"
        },
        {
          "type": "SYMBOL",
          "name": "_hor_space"
        },
        {
          "type": "SYMBOL",
          "name": "define_value"
        }
      ]
    },
    "define_key": {
      "type": "PATTERN",
      "value": "[a-zA-Z0-9_]+"
    },
    "define_value": {
      "type": "PATTERN",
      "value": "[^{}\\r\\n]+"
    },
    "label": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "#"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "_hor_space"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "SYMBOL",
          "name": "label_id"
        }
      ]
    },
    "label_id": {
      "type": "PATTERN",
      "value": "[a-zA-Z0-9_]+"
    },
    "command": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "@"
        },
        {
          "type": "SYMBOL",
          "name": "command_id"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "command_nameless_param"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "command_param"
          }
        }
      ]
    },
    "inlined_command": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "["
        },
        {
          "type": "SYMBOL",
          "name": "command_id"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "command_nameless_param"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "command_param"
          }
        },
        {
          "type": "STRING",
          "value": "]"
        }
      ]
    },
    "command_id": {
      "type": "PATTERN",
      "value": "[a-zA-Z0-9_]+"
    },
    "command_param": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_hor_space"
        },
        {
          "type": "SYMBOL",
          "name": "command_param_id"
        },
        {
          "type": "SYMBOL",
          "name": "command_param_value"
        }
      ]
    },
    "command_nameless_param": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_hor_space"
        },
        {
          "type": "SYMBOL",
          "name": "command_param_value"
        }
      ]
    },
    "command_param_id": {
      "type": "PREC_LEFT",
      "value": 0,
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "PATTERN",
            "value": "[a-zA-Z0-9_]+:"
          }
        ]
      }
    },
    "command_param_value": {
      "type": "PREC_RIGHT",
      "value": 0,
      "content": {
        "type": "REPEAT1",
        "content": {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "expression"
            },
            {
              "type": "CHOICE",
              "members": [
                {
                  "type": "PATTERN",
                  "value": "[^\"\\s:\\[\\]{}@]+"
                },
                {
                  "type": "SEQ",
                  "members": [
                    {
                      "type": "STRING",
                      "value": "\""
                    },
                    {
                      "type": "PATTERN",
                      "value": "(\\\\.|[^\"\\n\\[\\]{}])*"
                    },
                    {
                      "type": "STRING",
                      "value": "\""
                    }
                  ]
                }
              ]
            }
          ]
        }
      }
    },
    "_hor_space": {
      "type": "PATTERN",
      "value": "[ \\t]+"
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    }
  ],
  "conflicts": [],
  "externals": [],
  "inline": [],
  "supertypes": []
}

